{

"files.exclude": {
    "bin/": true,
    "obj/": true
},
"cSpell.words": [
    "csporj",
    "Ingame",
    "reksar's",
    "toolkits"
],
"[powershell]": {
    "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
